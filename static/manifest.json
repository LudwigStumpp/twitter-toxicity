{
    "name": "Twitter Toxicity Detector",
    "description": "A Toxicity Detector on Twitter",
    "version": "1.0",
    "manifest_version": 3,
    "content_scripts": [
        {
            "js": [
                "content.js"
            ],
            "matches": [
                "https://twitter.com/*"
            ]
        }
    ],
    "background": {
        "service_worker": "background.js"
    }
}